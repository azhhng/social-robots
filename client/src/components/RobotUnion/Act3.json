                                                                                                                                                                                                                            "speaker": "Bear",
                                                                                                                                                                                                                            "responses": 2,
                                                                                                                                                                                                                            "children": [
                                                                                                                                                                                                                                {
                                                                                                                                                                                                                                    "response": "enter",
                                                                                                                                                                                                                                    "text": "> Not so fast.",
                                                                                                                                                                                                                                    "speaker": "Developer",
                                                                                                                                                                                                                                    "responses": 1,
